<xs:schema xmlns:xs='http://www.w3.org/2001/XMLSchema'
           xmlns='urn:riv:clinicalprocess:activityprescription:prescribe:ConfirmPrescriptionResponder:1'
           xmlns:prescribe='urn:riv:clinicalprocess:activityprescription:prescribe:1'
           targetNamespace='urn:riv:clinicalprocess:activityprescription:prescribe:ConfirmPrescriptionResponder:1'
           elementFormDefault='qualified'
           attributeFormDefault='unqualified'
           version='1.0'>

    <xs:import schemaLocation='../../../schemas/core_components/clinicalprocess_activityprescription_prescribe_1.0.xsd'
               namespace='urn:riv:clinicalprocess:activityprescription:prescribe:1' />

    <xs:element name='ConfirmPrescription'
                type='ConfirmPrescriptionType' />

    <xs:element name='ConfirmPrescriptionResponse'
                type='ConfirmPrescriptionResponseType' />

    <xs:complexType name='ConfirmPrescriptionType'>
        <xs:sequence>

            <xs:element name="aktuellUnderlagstidpunkt"
                        type="xs:dateTime"
                        minOccurs="0"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Tidpunkt för senaste ordinationsändring för en patient.
Anges för patienter med befintligt underlag.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="underlagsversion"
                        type="xs:int"
                        minOccurs="0"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Underlagsversion från receptdepå för dospatienter.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="vardpersonal"
                        type="prescribe:VardpersonalInformation"
                        minOccurs="1"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Information om registrerande och ordinerande vårdpersonal.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="patient"
                        type="prescribe:Patient"
                        minOccurs="1"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Patientens identifikationsobjekt.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="samtyckeLasRecept"
                        type="xs:boolean"
                        minOccurs="1"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Patientens givna samtycke till att läsa ur receptdepån.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="receptId"
                        type="xs:long"
                        minOccurs="1"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Receptets id.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:choice>

                <xs:element name="insattningsinformation"
                            type="prescribe:Insattningsinformation"
                            minOccurs="0"
                            maxOccurs="1"
                            nillable="false">
                    <xs:annotation>
                        <xs:documentation>Insättning av ej verifierad ordination.</xs:documentation>
                    </xs:annotation>
                </xs:element>

                <xs:element name="utsattningsinformation"
                            type="prescribe:UtsattningsinformationConfirm"
                            minOccurs="0"
                            maxOccurs="1"
                            nillable="false">
                    <xs:annotation>
                        <xs:documentation>Utsättning av ej verifierad ordination.</xs:documentation>
                    </xs:annotation>
                </xs:element>

            </xs:choice>

            <xs:element name="ordination"
                        type="prescribe:LakemedelsordinationsunderlagConfirmBas"
                        minOccurs="1"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Ej verifierad ordination som skall journalföras.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="anvandareinformation"
                        type="prescribe:Anvandareinformation"
                        minOccurs="1"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Klientinformation innehållande anropande systems information.
Obligatorisk information vid anrop till Receptdepå human.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="klientinformation"
                        type="prescribe:Klientinformation"
                        minOccurs="1"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Klientinformation innehållande anropande systems information.
Obligatorisk information vid anrop till Receptdepå human.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:any namespace='##other'
                    processContents='lax'
                    minOccurs='0'
                    maxOccurs='unbounded' />

        </xs:sequence>
    </xs:complexType>

    <xs:complexType name='ConfirmPrescriptionResponseType'>
        <xs:sequence>

            <xs:element name="ordinationsId"
                        type="prescribe:UUID"
                        minOccurs="0"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Referens till ordination som verifierats.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="ordinationkedjeId"
                        type="prescribe:UUID"
                        minOccurs="0"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Referens till ordinationskedjan som skapats.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="uppdateradUnderlagstidpunkt"
                        type="xs:dateTime"
                        minOccurs="0"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Tidpunkt för uppdaterat underlag för patient.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="vokresultat"
                        type="prescribe:VOKResultat"
                        minOccurs="0"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Resultat från VOK-VER.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name='resultCode'
                        type='prescribe:resultCodeEnum'
                        minOccurs="1"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Resultatkod för anropet.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element minOccurs="0" maxOccurs="1" nillable="false" name="comment" type="comment"/>

            <xs:any namespace='##other'
                    processContents='lax'
                    minOccurs='0'
                    maxOccurs='unbounded' />

        </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="comment">
        <xs:annotation>
            <xs:documentation>Kommentar till eventuella fel eller varningar.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="1024"/>
        </xs:restriction>
    </xs:simpleType>

</xs:schema>