<xs:schema xmlns:xs='http://www.w3.org/2001/XMLSchema'
           xmlns='urn:riv:clinicalprocess:activityprescription:prescribe:ChangePrescriptionResponder:1'
           xmlns:prescribe='urn:riv:clinicalprocess:activityprescription:prescribe:1'
           targetNamespace='urn:riv:clinicalprocess:activityprescription:prescribe:ChangePrescriptionResponder:1'
           elementFormDefault='qualified'
           attributeFormDefault='unqualified'
           version='1.0'>

    <xs:import schemaLocation='../../../schemas/core_components/clinicalprocess_activityprescription_prescribe_1.0.xsd'
               namespace='urn:riv:clinicalprocess:activityprescription:prescribe:1' />

    <xs:element name='ChangePrescription'
                type='ChangePrescriptionType' />

    <xs:element name='ChangePrescriptionResponse'
                type='ChangePrescriptionResponseType' />

    <xs:complexType name='ChangePrescriptionType'>
        <xs:sequence>

            <xs:element name="aktuellUnderlagstidpunkt"
                        type="xs:dateTime"
                        minOccurs="1"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Tidpunkt för senaste ordinationsändring för en patient.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="underlagsversion"
                        type="xs:int"
                        minOccurs="0"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Underlagsversion från receptdepå för dospatienter.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="beslutstidpunkt"
                        type="xs:dateTime"
                        minOccurs="0"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Tidpunkt för upprättande av beslut.
Om ej angiven antas momentan tidpunkt.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="vardpersonal"
                        type="prescribe:VardpersonalInformation"
                        minOccurs="1"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Information om registrerande och ordinerande vårdpersonal.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="forandrandeForskrivare"
                        type="prescribe:Forskrivareinformation"
                        minOccurs="0"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Förändrande förskrivare.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="forskrivarensKontaktuppgift"
                        type="prescribe:Kontaktuppgift"
                        minOccurs="0"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Förskrivarens kontaktuppgifter.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:choice  minOccurs = "0"
                        maxOccurs = "1">
                <xs:element name = "forskrivningsmakulering"
                            type = "prescribe:ForskrivningsmakuleringsunderlagBas"/>
                <xs:element name = "forskrivningsutsattning"
                            type = "prescribe:ForskrivningsutsattningsunderlagBas"/>
            </xs:choice>

            <xs:element minOccurs="1" maxOccurs="1" nillable="false" name="ordinationsId" type="utsattningsordinationsId"/>

            <xs:element name="utsattning"
                        type="prescribe:UtsattningsunderlagBas"
                        minOccurs="1"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Utsättningsinformation.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="expeditionsunderlag"
                        type="prescribe:ForskrivningsinsattningBas"
                        minOccurs="0"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Underlag för förskrivningsinsättning.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="insattning"
                        type="prescribe:InsattningsunderlagBas"
                        minOccurs="1"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Insättningsinformation.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="ordination"
                        type="prescribe:LakemedelsordinationsunderlagBas"
                        minOccurs="1"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Ordinationsinformation.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="patient"
                        type="prescribe:Patient"
                        minOccurs="1"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Patientens identifikationsobjekt.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="klientinformation"
                        type="prescribe:Klientinformation"
                        minOccurs="0"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Klientinformation innehållande anropande systems information.
Obligatorisk information vid anrop till Receptdepå human.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:any namespace='##other'
                    processContents='lax'
                    minOccurs='0'
                    maxOccurs='unbounded' />

        </xs:sequence>
    </xs:complexType>

    <xs:complexType name='ChangePrescriptionResponseType'>
        <xs:sequence>

            <xs:element minOccurs="0" maxOccurs="1" nillable="false" name="ordinationsId" type="ordinationsId"/>

            <xs:element name="uppdateradUnderlagstidpunkt"
                        type="xs:dateTime"
                        minOccurs="0"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Tidpunkt för uppdaterat underlag för patient.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="vokresultat"
                        type="prescribe:VOKResultat"
                        minOccurs="0"
                        maxOccurs="3"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Resultat från VOK-INS, VOK-UTS.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name='resultCode'
                        type='prescribe:resultCodeEnum'
                        minOccurs="1"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Resultatkod för anropet.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element minOccurs="0" maxOccurs="1" nillable="false" name="comment" type="comment"/>

            <xs:any namespace='##other'
                    processContents='lax'
                    minOccurs='0'
                    maxOccurs='unbounded' />

        </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="utsattningsordinationsId">
        <xs:annotation>
            <xs:documentation>Referens till ordination att sätta ut.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="36"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ordinationsId">
        <xs:annotation>
            <xs:documentation>Referens till ordination som skapats.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="36"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="comment">
        <xs:annotation>
            <xs:documentation>Kommentar till eventuella fel eller varningar.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="1024"/>
        </xs:restriction>
    </xs:simpleType>

</xs:schema>