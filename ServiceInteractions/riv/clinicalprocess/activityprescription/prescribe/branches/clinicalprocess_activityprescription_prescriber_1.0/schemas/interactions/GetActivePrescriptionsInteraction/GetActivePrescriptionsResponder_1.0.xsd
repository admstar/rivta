<xs:schema xmlns:xs='http://www.w3.org/2001/XMLSchema'
           xmlns='urn:riv:clinicalprocess:activityprescription:prescribe:GetActivePrescriptionsResponder:1'
           xmlns:prescribe='urn:riv:clinicalprocess:activityprescription:prescribe:1'
           targetNamespace='urn:riv:clinicalprocess:activityprescription:prescribe:GetActivePrescriptionsResponder:1'
           elementFormDefault='qualified'
           attributeFormDefault='unqualified'
           version='1.0'>

    <xs:import schemaLocation='../../../schemas/core_components/clinicalprocess_activityprescription_prescribe_1.0.xsd'
               namespace='urn:riv:clinicalprocess:activityprescription:prescribe:1' />

    <xs:element name='GetActivePrescriptions'
                type='GetActivePrescriptionsType' />

    <xs:element name='GetActivePrescriptionsResponse'
                type='GetActivePrescriptionsResponseType'/>

    <xs:complexType name='GetActivePrescriptionsType'>
        <xs:sequence>

            <xs:element name="patient"
                        type="prescribe:Patient"
                        minOccurs="1"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Patient att hämta aktuella ordinationer för.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="svarsurval"
                        type="prescribe:Svarsurval"
                        minOccurs="0"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Urval som begränsar antal returnerade poster. Om svarsurval ej är
angivet begränsas antal poster till de 100 första.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="varainformation"
                        type="xs:boolean"
                        default="true"
                        minOccurs="0"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Anger om svaret ska kompletteras med information från Produkt- och artikelregistret.
Se kompletterandeHandelsvaruinformation, kompletterandeLakemedelsartikelinformation
och kompletterandeLakemedelsproduktinformation.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:any namespace='##other'
                    processContents='lax'
                    minOccurs='0'
                    maxOccurs='unbounded' />

        </xs:sequence>
    </xs:complexType>

    <xs:complexType name='GetActivePrescriptionsResponseType'>
        <xs:sequence>

            <xs:element name="underlagstidpunkt"
                        type="xs:dateTime"
                        minOccurs="0"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Tidpunkt för senaste ordinationsändring för en patient. Returneras endast
om patienten finns i NOD.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="lakemedelsordination"
                        type="prescribe:Lakemedelsordination"
                        minOccurs="0"
                        maxOccurs="unbounded"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Aktuella ordinationer för angiven patient.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="kompletterandeHandelsvaruinformation"
                        type="prescribe:Handelsvaruinformation"
                        minOccurs="0"
                        maxOccurs="unbounded"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Eventuell kompletterande handelsvaruinformation.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="kompletterandeLakemedelsartikelinformation"
                        type="prescribe:Lakemedelsartikelinformation"
                        minOccurs="0"
                        maxOccurs="unbounded"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Eventuell kompletterande läkemedelsartikelinformation.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="kompletterandeLakemedelsproduktinformation"
                        type="prescribe:Lakemedelsproduktinformation"
                        minOccurs="0"
                        maxOccurs="unbounded"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Eventuell kompletterande läkemedelsproduktinformation.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="svarsurval"
                        type="prescribe:Svarsurval"
                        minOccurs="0"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Svarsurval returneras om svarsurval är angivet i begäran eller om svarsurval
inte är angivet och totalt antal poster är större än 100.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name="totaltAntalPoster"
                        type="xs:int"
                        minOccurs="0"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Totalt antal poster.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element name='resultCode'
                        type='prescribe:resultCodeEnum'
                        minOccurs="1"
                        maxOccurs="1"
                        nillable="false">
                <xs:annotation>
                    <xs:documentation>Resultatkod för anropet.</xs:documentation>
                </xs:annotation>
            </xs:element>

            <xs:element minOccurs="0" maxOccurs="1" nillable="false" name="comment" type="comment"/>

            <xs:any namespace='##other'
                    processContents='lax'
                    minOccurs='0'
                    maxOccurs='unbounded' />

        </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="comment">
        <xs:annotation>
            <xs:documentation>Kommentar till eventuella fel eller varningar.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="1024"/>
        </xs:restriction>
    </xs:simpleType>

</xs:schema>
