<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>se.rivta.tools</groupId>
  <artifactId>clinicalprocess-healthcond-description</artifactId>
  <packaging>jar</packaging>
  <version>2.1</version>
  <name>clinicalprocess-healthcond-description-schemas</name>
  <url>http://maven.apache.org</url>
  <properties>
    <schema.path>${basedir}/../../schemas</schema.path>
    <cxf.version>2.7.14</cxf.version>
	<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
  <dependencies/>
  <pluginRepositories>
    <pluginRepository>
      <id>repository.codehaus.org</id>
      <url>http://repository.codehaus.org/</url>
    </pluginRepository>
  </pluginRepositories>
  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.cxf</groupId>
        <artifactId>cxf-codegen-plugin</artifactId>
        <version>${cxf.version}</version>
        <executions>
          <execution>
            <id>generate-sources</id>
            <phase>generate-sources</phase>
            <configuration>
			<fork>once</fork>
			<additionalJvmArgs>-Dfile.encoding=UTF-8</additionalJvmArgs>
              <sourceRoot>${basedir}/target/generated/src/main/java</sourceRoot>
				<validate>true</validate>
              <wsdlOptions>
                <wsdlOption>
                  <wsdl>${schema.path}/interactions/GetCareDocumentationInteraction/GetCareDocumentationInteraction_2.1_RIVTABP21.wsdl</wsdl>
                  <extraargs>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:clinicalprocess:healthcond:description:GetCareDocumentation:2:rivtabp21=se.riv.clinicalprocess.healthcond.description.getcaredocumentation.v2</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:clinicalprocess:healthcond:description:GetCareDocumentationResponder:2=se.riv.clinicalprocess.healthcond.description.getcaredocumentationresponder.v2</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:clinicalprocess:healthcond:description:GetCareDocumentationInitiator:2=se.riv.clinicalprocess.healthcond.description.getcaredocumentationinitiator.v2</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:itintegration:registry:1=se.riv.itintegration.registry.v1</extraarg>
                  </extraargs>
                </wsdlOption>								

                <wsdlOption>
                  <wsdl>${schema.path}/interactions/GetAlertInformationInteraction/GetAlertInformationInteraction_2.0_RIVTABP21.wsdl</wsdl>
                  <extraargs>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:clinicalprocess:healthcond:description:GetAlertInformation:2:rivtabp21=se.riv.clinicalprocess.healthcond.description.getalertinformation.v2</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:clinicalprocess:healthcond:description:GetAlertInformationResponder:2=se.riv.clinicalprocess.healthcond.description.getalertinformationresponder.v2</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:clinicalprocess:healthcond:description:GetAlertInformationInitiator:2=se.riv.clinicalprocess.healthcond.description.getalertinformationinitiator.v2</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:itintegration:registry:1=se.riv.itintegration.registry.v1</extraarg>
                  </extraargs>
                </wsdlOption>								

                <wsdlOption>
                  <wsdl>${schema.path}/interactions/GetDiagnosisInteraction/GetDiagnosisInteraction_2.0_RIVTABP21.wsdl</wsdl>
                  <extraargs>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:clinicalprocess:healthcond:description:GetDiagnosis:2:rivtabp21=se.riv.clinicalprocess.healthcond.description.getdiagnosis.v2</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:clinicalprocess:healthcond:description:GetDiagnosisResponder:2=se.riv.clinicalprocess.healthcond.description.getdiagnosisresponder.v2</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:clinicalprocess:healthcond:description:GetDiagnosisInitiator:2=se.riv.clinicalprocess.healthcond.description.getdiagnosisinitiator.v2</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:itintegration:registry:1=se.riv.itintegration.registry.v1</extraarg>
                  </extraargs>
                </wsdlOption>								

                <wsdlOption>
                  <wsdl>${schema.path}/interactions/GetFunctionalStatusInteraction/GetFunctionalStatusInteraction_2.0_RIVTABP21.wsdl</wsdl>
                  <extraargs>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:clinicalprocess:healthcond:description:GetFunctionalStatus:2:rivtabp21=se.riv.clinicalprocess.healthcond.description.getfunctionalstatus.v2</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:clinicalprocess:healthcond:description:GetFunctionalStatusResponder:2=se.riv.clinicalprocess.healthcond.description.getfunctionalstatusresponder.v2</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:clinicalprocess:healthcond:description:GetFunctionalStatusInitiator:2=se.riv.clinicalprocess.healthcond.description.getfunctionalstatusinitiator.v2</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:itintegration:registry:1=se.riv.itintegration.registry.v1</extraarg>
                  </extraargs>
                </wsdlOption>								

              </wsdlOptions>
            </configuration>
            <goals>
              <goal>wsdl2java</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
		  <version>3.2</version>
        <configuration>
          <source>1.7</source>
          <target>1.7</target>
        </configuration>
      </plugin>

    </plugins>
    <resources>
      <resource>
        <directory>${schema.path}</directory>
      </resource>
    </resources>
  </build>
</project>
