<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>se.rivta.tools</groupId>
    <artifactId>clinicalprocess-healthcond-actoutcome-schemas</artifactId>
    <packaging>jar</packaging>
    <version>1.0-SNAPSHOT</version>
    <name>clinicalprocess-healthcond-actoutcome</name>
    <url>http://maven.apache.org</url>
    <properties>
        <schema.path>${basedir}/../schemas</schema.path>
        <cxf.version>2.6.1</cxf.version>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    </properties>
    <dependencies/>
    <pluginRepositories>
        <pluginRepository>
            <id>repository.codehaus.org</id>
            <url>http://repository.codehaus.org/</url>
        </pluginRepository>
    </pluginRepositories>
    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.cxf</groupId>
                <artifactId>cxf-codegen-plugin</artifactId>
                <version>${cxf.version}</version>
                <executions>
                    <execution>
                        <id>generate-sources</id>
                        <phase>generate-sources</phase>
                        <configuration>
                            <fork>once</fork>
                            <additionalJvmArgs>-Dfile.encoding=UTF-8</additionalJvmArgs>
                            <sourceRoot>${basedir}/target/generated/src/main/java</sourceRoot>
                            <validate>true</validate>
                            <wsdlOptions>
                                <!-- GetReferralOutcome -->
                                <wsdlOption>
                                    <extraargs>
                                        <extraarg>-p</extraarg>
                                        Note: be careful when changing this - must be in sync for all schemas generations in this namespace!										 
                                        <extraarg>
                                            urn:riv:clinicalprocess:healthcond:actoutcome:GetReferralOutcomeRequest:1:rivtabp21=se.riv.clinicalprocess.healthcond.actoutcome.processrequest.v1
                                        </extraarg>
                                        <extraarg>-p</extraarg>
                                        <extraarg>
                                            urn:riv:clinicalprocess:healthcond:actoutcome:GetReferralOutcomeResponse:1=se.riv.clinicalprocess.healthcond.actoutcome.processrequest.v1
                                        </extraarg>
                                        <extraarg>-p</extraarg>
                                        <extraarg>
                                            urn:riv:clinicalprocess:healthcond:actoutcome:1=se.riv.clinicalprocess.healthcond.actoutcome.v1
                                        </extraarg>
                                        <extraarg>-p</extraarg>
                                        <extraarg>
                                            urn:riv:itintegration:registry:1=se.riv.itintegration.registry.v1
                                        </extraarg>
                                    </extraargs>
                                    <wsdl>${schema.path}/interactions/GetReferralOutcomeInteraction/GetReferralOutcomeInteraction_2.0_RIVTABP21.wsdl</wsdl>
                                </wsdlOption>
                                
                                <!-- GetPrenatalMedicalHistory -->
                                <wsdlOption>
                                    <extraargs>
                                        <extraarg>-p</extraarg>
                                        Note: be careful when changing this - must be in sync for all schemas generations in this namespace!										 
                                        <extraarg>
                                            urn:riv:clinicalprocess:healthcond:actoutcome:GetPrenatalMedicalHistoryRequest:1:rivtabp21=se.riv.clinicalprocess.healthcond.actoutcome.processrequest.v1
                                        </extraarg>
                                        <extraarg>-p</extraarg>
                                        <extraarg>
                                            urn:riv:clinicalprocess:healthcond:actoutcome:GetPrenatalMedicalHistoryResponse:1=se.riv.clinicalprocess.healthcond.actoutcome.processrequest.v1
                                        </extraarg>
                                        <extraarg>-p</extraarg>
                                        <extraarg>
                                            urn:riv:clinicalprocess:healthcond:actoutcome:1=se.riv.clinicalprocess.healthcond.actoutcome.v1
                                        </extraarg>
                                        <extraarg>-p</extraarg>
                                        <extraarg>
                                            urn:riv:itintegration:registry:1=se.riv.itintegration.registry.v1
                                        </extraarg>
                                    </extraargs>
                                    <wsdl>${schema.path}/interactions/GetPrenatalMedicalHistoryInteraction/GetPrenatalMedicalHistoryInteraction_2.0_RIVTABP21.wsdl</wsdl>
                                </wsdlOption>								
                                
                                <!-- GetDeliveryMedicalHistory -->
                                <wsdlOption>
                                    <extraargs>
                                        <extraarg>-p</extraarg>
                                        Note: be careful when changing this - must be in sync for all schemas generations in this namespace!										 
                                        <extraarg>
                                            urn:riv:clinicalprocess:healthcond:actoutcome:GetDeliveryMedicalHistoryRequest:1:rivtabp21=se.riv.clinicalprocess.healthcond.actoutcome.processrequest.v1
                                        </extraarg>
                                        <extraarg>-p</extraarg>
                                        <extraarg>
                                            urn:riv:clinicalprocess:healthcond:actoutcome:GetDeliveryMedicalHistoryResponse:1=se.riv.clinicalprocess.healthcond.actoutcome.processrequest.v1
                                        </extraarg>
                                        <extraarg>-p</extraarg>
                                        <extraarg>
                                            urn:riv:clinicalprocess:healthcond:actoutcome:1=se.riv.clinicalprocess.healthcond.actoutcome.v1
                                        </extraarg>
                                        <extraarg>-p</extraarg>
                                        <extraarg>
                                            urn:riv:itintegration:registry:1=se.riv.itintegration.registry.v1
                                        </extraarg>
                                    </extraargs>
                                    <wsdl>${schema.path}/interactions/GetDeliveryMedicalHistoryInteraction/GetDeliveryMedicalHistoryInteraction_2.0_RIVTABP21.wsdl</wsdl>
                                </wsdlOption>								
                            </wsdlOptions>
 
                        </configuration>
                        <goals>
                            <goal>wsdl2java</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>2.5.1</version>
                <configuration>
                    <source>1.6</source>
                    <target>1.6</target>
                </configuration>
            </plugin>

        </plugins>
        <resources>
            <resource>
                <directory>${schema.path}</directory>
            </resource>
        </resources>
    </build>
</project>
