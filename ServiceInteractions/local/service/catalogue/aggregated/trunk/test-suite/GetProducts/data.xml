<testsuite>
    <id>GetProducts</id>
    <description>This testsuite is designed to verify the GetProducts contract.</description>
    <globaldata>
        <webServiceUrl>http://localhost:8088/mockGetProductsResponderBinding</webServiceUrl>
    </globaldata>
    <testcase id="Get all products for county">
        <description>Verifies that the result only contains information for the given patient.<br/>
        </description>
        <data>
            <logicalAddress>112233</logicalAddress>
            <county>01</county>
        </data>
    </testcase>
 
    <testcase id="Get all products for sourceSystemHSAId">
        <description> </description>
        <data>
            <logicalAddress>112233</logicalAddress>
            <sourceSystemHSAId>SE-123456</sourceSystemHSAId>
        </data>
    </testcase>
    <testcase id="Get all products">
        <description>
        </description>
        <data>
            <logicalAddress>112233</logicalAddress>
        </data>
    </testcase>
</testsuite>
