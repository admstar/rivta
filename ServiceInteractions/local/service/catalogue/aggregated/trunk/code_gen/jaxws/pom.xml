<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>se.rivta.tools</groupId>
  <artifactId>service-catalogue-schemas</artifactId>
  <packaging>jar</packaging>
  <version>2.0</version>
  <name>service-catalogue-schemas</name>
  <url>http://maven.apache.org</url>
  <properties>
    <schema.path>${basedir}/../../schemas</schema.path>
    <cxf.version>3.0.4</cxf.version>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
  <dependencies/>
  <pluginRepositories>
    <pluginRepository>
      <id>repository.codehaus.org</id>
      <url>http://repository.codehaus.org/</url>
    </pluginRepository>
  </pluginRepositories>
  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.cxf</groupId>
        <artifactId>cxf-codegen-plugin</artifactId>
        <version>${cxf.version}</version>
        <executions>
          <execution>
            <id>generate-sources</id>
            <phase>generate-sources</phase>
            <configuration>
              <sourceRoot>${basedir}/target/generated/src/main/java</sourceRoot>
              <wsdlOptions>

		<!-- FindOffers -->
                <wsdloption>
                  <wsdl>${schema.path}/interactions/FindOffersInteraction/FindOffersInteraction_2.0_RIVTABP21.wsdl</wsdl>
                  <extraargs>
                    <extraarg>-p</extraarg>
                    <extraarg>http://www.w3.org/2005/08/addressing=org.w3c.addressing.v1</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:service:catalogue:FindOffersResponder:2=se.riv.service.catalogue.findoffersresponder.v2</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:service:catalogue:FindOffersInitiator:2=se.riv.service.catalogue.findoffersinitiator.v2</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:service:catalogue:FindOffers:2:rivtabp21=se.riv.service.catalogue.findoffers.v2.rivtabp21</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:itintegration:registry:1=se.riv.itintegration.registry.v1</extraarg>
                  </extraargs>
                </wsdloption>

		<!-- GetCapacities -->
                <wsdloption>
                  <wsdl>${schema.path}/interactions/GetCapacitiesInteraction/GetCapacitiesInteraction_1.0_RIVTABP21.wsdl</wsdl>
                  <extraargs>
                    <extraarg>-p</extraarg>
                    <extraarg>http://www.w3.org/2005/08/addressing=org.w3c.addressing.v1</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:service:catalogue:GetCapacitiesResponder:1=se.riv.service.catalogue.getcapacitiesresponder.v1</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:service:catalogue:GetCapacitiesInitiator:1=se.riv.service.catalogue.getcapacitiesinitiator.v1</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:service:catalogue:GetCapacities:1:rivtabp21=se.riv.service.catalogue.getcapacities.v1.rivtabp21</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:itintegration:registry:1=se.riv.itintegration.registry.v1</extraarg>
                  </extraargs>
                </wsdloption>

		<!-- PullOffers -->
                <wsdloption>
                  <wsdl>${schema.path}/interactions/PullOffersInteraction/PullOffersInteraction_1.0_RIVTABP21.wsdl</wsdl>
                  <extraargs>
                    <extraarg>-p</extraarg>
                    <extraarg>http://www.w3.org/2005/08/addressing=org.w3c.addressing.v1</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:service:catalogue:PullOffersResponder:1=se.riv.service.catalogue.pulloffersresponder.v1</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:service:catalogue:PullOffersInitiator:1=se.riv.service.catalogue.pulloffersinitiator.v1</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:service:catalogue:PullOffers:1:rivtabp21=se.riv.service.catalogue.pulloffers.v1.rivtabp21</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:itintegration:registry:1=se.riv.itintegration.registry.v1</extraarg>
                  </extraargs>
                </wsdloption>

			<!-- GetProducts -->
                <wsdloption>
                  <wsdl>${schema.path}/interactions/GetProductsInteraction/GetProductsInteraction_2.0_RIVTABP21.wsdl</wsdl>
                  <extraargs>
                    <extraarg>-p</extraarg>
                    <extraarg>http://www.w3.org/2005/08/addressing=org.w3c.addressing.v1</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:service:catalogue:GetProductsResponder:2=se.riv.service.catalogue.getproductsresponder.v2</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:service:catalogue:GetProductsInitiator:2=se.riv.service.catalogue.getproductsinitiator.v2</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:service:catalogue:GetProducts:2:rivtabp21=se.riv.service.catalogue.getproducts.v2.rivtabp21</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:itintegration:registry:1=se.riv.itintegration.registry.v1</extraarg>
                  </extraargs>
                </wsdloption>

			<!-- UpdateOffers -->
                <wsdloption>
                  <wsdl>${schema.path}/interactions/UpdateOffersInteraction/UpdateOffersInteraction_1.0_RIVTABP21.wsdl</wsdl>
                  <extraargs>
                    <extraarg>-p</extraarg>
                    <extraarg>http://www.w3.org/2005/08/addressing=org.w3c.addressing.v1</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:service:catalogue:UpdateOffersResponder:1=se.riv.service.catalogue.updateoffersresponder.v1</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:service:catalogue:UpdateOffersInitiator:1=se.riv.service.catalogue.updateoffersinitiator.v1</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:service:catalogue:UpdateOffers:1:rivtabp21=se.riv.service.catalogue.updateoffers.v1.rivtabp21</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:itintegration:registry:1=se.riv.itintegration.registry.v1</extraarg>
                  </extraargs>
                </wsdloption>

 			<!-- UpdateProducts -->
                <wsdloption>
                  <wsdl>${schema.path}/interactions/UpdateProductsInteraction/UpdateProductsInteraction_1.0_RIVTABP21.wsdl</wsdl>
                  <extraargs>
                    <extraarg>-p</extraarg>
                    <extraarg>http://www.w3.org/2005/08/addressing=org.w3c.addressing.v1</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:service:catalogue:UpdateProductsResponder:1=se.riv.service.catalogue.updateproductsresponder.v1</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:service:catalogue:UpdateProductsInitiator:1=se.riv.service.catalogue.updateproductsinitiator.v1</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:service:catalogue:UpdateProducts:1:rivtabp21=se.riv.service.catalogue.updateproducts.v1.rivtabp21</extraarg>
                    <extraarg>-p</extraarg>
                    <extraarg>urn:riv:itintegration:registry:1=se.riv.itintegration.registry.v1</extraarg>
                  </extraargs>
                </wsdloption>
 
              </wsdlOptions>
            </configuration>
            <goals>
              <goal>wsdl2java</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
		<version>3.2</version>
        <configuration>
          <source>1.7</source>
          <target>1.7</target>
          <encoding>UTF-8</encoding>
        </configuration>
      </plugin>
    </plugins>
    <resources>
      <resource>
        <directory>${schema.path}</directory>
      </resource>
    </resources>
  </build>
</project>
